<Window x:Class="PCHealthDashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PC Health Dashboard" Height="300" Width="400">
    <Grid Margin="20">
        <StackPanel>
            <TextBlock x:Name="CpuText" FontSize="16" Margin="0 10" />
            <TextBlock x:Name="RamText" FontSize="16" Margin="0 10" />
            <TextBlock x:Name="UptimeText" FontSize="16" Margin="0 10" />
        </StackPanel>
    </Grid>
</Window>
